{
  "version": "0.0.1",
  "name": "react-redux-boilerplate",
  "title": "React Redux Boilerplate",
  "description": "React with Redux Single-page application boilerplate",
  "author": "Alvear Candia, Cristopher Alejandro <calvear93@gmail.com>",
  "license": "GPL3",
  "private": true,
  "keywords": [
    "react",
    "react-redux",
    "boilerplate"
  ],
  "repository": "https://github.com/calvear93/react-redux-boilerplate",
  "dependencies": {
    "@calvear/react-azure-msal-security": "^2.0.1",
    "@calvear/react-redux": "^1.1.0",
    "@calvear/react-spa-routerizer": "^1.0.11",
    "@mdi/font": "^5.9.55",
    "@tippyjs/react": "^4.2.4",
    "axios": "^0.21.1",
    "axios-hooks": "^2.5.0",
    "beautiful-react-hooks": "^0.31.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.19.0",
    "devextreme": "^20.2.5",
    "devextreme-react": "^20.2.5",
    "http-status-codes": "^2.1.4",
    "imask": "^6.0.7",
    "mime-types": "^2.1.29",
    "mout": "^1.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-flexbox-grid": "^2.1.2",
    "react-imask": "^6.0.7",
    "react-modern-calendar-datepicker": "^3.1.6",
    "react-router-dynamic-breadcrumbs": "^2.2.0",
    "react-spinners": "^0.10.6",
    "react-timekeeper": "^2.1.3",
    "react-toastify": "^7.0.3",
    "react-use": "^17.2.1",
    "rxjs": "^6.6.6",
    "rxjs-hooks": "^0.6.2",
    "semantic-ui-react": "^2.0.3",
    "semantic-ui-sass": "^2.4.2",
    "store2": "^2.12.0",
    "sweetalert2": "^10.15.5",
    "sweetalert2-react-content": "^3.3.1",
    "swr": "^0.5.3",
    "uuid": "^8.3.2",
    "validate.js": "^0.13.1",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@craco/craco": "^6.1.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "craco-image-optimizer-plugin": "0.0.4",
    "env-cmd": "^10.1.0",
    "eslint": "^7.22.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.2.1",
    "eslint-plugin-jsdoc": "^32.2.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-radar": "^0.2.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-react-redux": "^3.3.2",
    "jest": "^26.6.3",
    "miragejs": "^0.1.41",
    "node-sass": "^7.0.0",
    "prettier-stylelint": "^0.4.2",
    "react-scripts": "^4.0.3",
    "stylelint": "^13.12.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "webpack-bundle-analyzer": "^4.4.0"
  },
  "cracoConfig": "webpack.config.js",
  "scripts": {
    "start:debug": "env-cmd -e default,debug -r .env.js craco start --verbose",
    "start:development": "env-cmd -e default,development -r .env.js craco start",
    "start:qa": "env-cmd -e default,qa -r .env.js craco start",
    "start:stage": "env-cmd -e default,stage -r .env.js craco start",
    "bundle:analyzer": "node webpack/bundle-analyzer.js",
    "build:development": "env-cmd -e default,build,development -r .env.js craco build -- --profile",
    "build:qa": "env-cmd -e default,build,qa -r .env.js craco build",
    "build:stage": "env-cmd -e default,build,stage -r .env.js craco build",
    "build:production": "env-cmd -e default,build,production -r .env.js craco build",
    "eslint:analyze": "eslint --ext .js,.jsx src/",
    "eslint:fix": "eslint --fix --ext .js,.jsx src/",
    "test:development": "env-cmd -e default,development,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:qa": "env-cmd -e default,qa,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:stage": "env-cmd -e default,stage,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:production": "env-cmd -e default,production,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:coverage": "react-scripts test --coverage",
    "postbuild": "rimraf build/static/**/*.map",
    "_npm:rebuild": "npm cache clean --force && npm rebuild && npm install",
    "_node-sass:rebuild": "npm rebuild node-sass",
    "_packages:update": "npm update --save/--save-dev",
    "_packages:force-update": "npx npm-check-updates -u",
    "_git:case-sensitive": "git config core.ignorecase false",
    "docker:build": "docker build --build-arg ENV=development --tag docker-image .",
    "docker:start": "docker run -d -it -p 3000:80/tcp --name docker-image-instance docker-image"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 8"
    ]
  }
}
