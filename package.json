    "uuid": "^8.0.0",
    "validate.js": "^0.13.1",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "env-cmd": "^10.1.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "eslint-plugin-react-redux": "^3.0.3",
    "prettier-stylelint": "^0.4.2",
    "react-scripts": "^3.4.1",
    "stylelint": "^13.3.3",
    "stylelint-order": "^4.0.0",
    "stylelint-scss": "^3.17.2"
  },
  "cracoConfig": "webpack.config.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start:debug": "env-cmd -e default,debug craco start --verbose",
    "start:development": "env-cmd -e default,development craco start",
    "start:qa": "env-cmd -e default,qa craco start",
    "build:development": "env-cmd -e default,development craco build",
    "build:qa": "env-cmd -e default,qa craco build",
    "build:production": "env-cmd -e default,production craco build",
    "lint": "eslint --fix --ext .js,.jsx ./src",
    "postbuild": "rimraf build/static/js/*.map",
    "packages:update": "npm update --save/--save-dev",
    "packages:update-force": "npm update --save/--save-dev -f"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 8"
    ]
  }
}
