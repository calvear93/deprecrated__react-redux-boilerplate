    "mime-types": "^2.1.27",
    "moment": "^2.25.3",
    "mout": "^1.2.2",
    "msal": "^1.3.1",
    "node-sass": "^4.14.1",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flexbox-grid": "^2.1.2",
    "react-imask": "^6.0.3",
    "react-redux": "^7.2.0",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-spinners": "^0.8.3",
    "react-timekeeper": "^2.1.1",
