{
  "version": "0.0.1",
  "name": "react-redux-boilerplate",
  "description": "React with Redux Single-page application boilerplate",
  "author": "Alvear Candia, Cristopher Alejandro <calvear93@gmail.com>",
  "license": "GPL3",
  "private": true,
  "keywords": [
    "react",
    "react-redux",
    "boilerplate"
  ],
  "repository": "https://github.com/calvear93/react-redux-boilerplate",
  "dependencies": {
    "@tippyjs/react": "^4.1.0",
    "axios": "^0.20.0",
    "beautiful-react-hooks": "^0.28.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.16.0",
    "devextreme": "^20.1.7",
    "devextreme-react": "^20.1.7",
    "http-status-codes": "^2.1.2",
    "imask": "^6.0.5",
    "mime-types": "^2.1.27",
    "mout": "^1.2.2",
    "msal": "^1.4.0",
    "node-sass": "^4.14.1",
    "query-string": "^6.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-flexbox-grid": "^2.1.2",
    "react-imask": "^6.0.5",
    "react-modern-calendar-datepicker": "^3.1.6",
    "react-redux": "^7.2.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-router-dynamic-breadcrumbs": "^2.2.0",
    "react-spinners": "^0.9.0",
    "react-timekeeper": "^2.1.1",
    "react-toastify": "^6.0.8",
    "react-use": "^15.3.3",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "rxjs": "^6.6.2",
    "semantic-ui-react": "^1.2.1",
    "semantic-ui-sass": "^2.4.2",
    "store2": "^2.12.0",
    "sweetalert2": "^9.17.1",
    "sweetalert2-react-content": "^3.0.3",
    "swr": "^0.3.1",
    "uuid": "^8.3.0",
    "validate.js": "^0.13.1",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@craco/craco": "^5.6.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^10.4.9",
    "@testing-library/user-event": "^12.1.3",
    "craco-image-optimizer-plugin": "0.0.3",
    "env-cmd": "^10.1.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-react-hooks": "^4.1.0",
    "eslint-plugin-react-redux": "^3.2.0",
    "jest": "^26.4.2",
    "miragejs": "^0.1.40",
    "prettier-stylelint": "^0.4.2",
    "react-scripts": "^3.4.3",
    "stylelint": "^13.6.1",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "webpack-bundle-analyzer": "^3.8.0"
  },
  "cracoConfig": "webpack.config.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "scripts": {
    "start:debug": "env-cmd -e default,debug -r .env.js craco start --verbose",
    "start:development": "env-cmd -e default,development -r .env.js craco start",
    "start:qa": "env-cmd -e default,qa -r .env.js craco start",
    "start:staging": "env-cmd -e default,staging -r .env.js craco start",
    "eslint:analyze": "eslint --ext .js,.jsx src/",
    "eslint:fix": "eslint --fix --ext .js,.jsx src/",
    "test:development": "env-cmd -e default,development,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:qa": "env-cmd -e default,qa,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:staging": "env-cmd -e default,staging,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:production": "env-cmd -e default,production,test -r .env.js react-scripts test --watchAll=false --verbose",
    "test:coverage": "react-scripts test --coverage",
    "bundle:analyzer": "node webpack/bundle-analyzer.js",
    "build:development": "env-cmd -e default,build,development -r .env.js craco build -- --profile",
    "build:qa": "env-cmd -e default,build,qa -r .env.js craco build",
    "build:staging": "env-cmd -e default,build,staging -r .env.js craco build",
    "build:production": "env-cmd -e default,build,production -r .env.js craco build",
    "postbuild": "rimraf build/static/**/*.map",
    "packages:update": "npm update --save/--save-dev",
    "packages:force-update": "npx npm-check-updates -u",
    "git:case-sensitive": "git config core.ignorecase false"
  },
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all",
      "not ie <= 8"
    ]
  }
}
